{
  "name": "@adonisjs/validator",
  "version": "6.2.5",
  "description": "Validator for adonis framework",
  "main": "build/providers/ValidatorProvider.js",
  "files": [
    "build/adonis-typings",
    "build/providers",
    "build/src"
  ],
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "clean": "del build",
    "compile": "npm run lint && npm run clean && tsc",
    "copy:files": "copyfiles \"templates/**/*.txt\" build",
    "build": "npm run compile",
    "prepublishOnly": "npm run build",
    "commit": "git-cz",
    "release": "np",
    "version": "npm run build",
    "lint": "eslint . --ext=.ts",
    "benchmarks": "node build/benchmarks/index.js"
  },
  "types": "./build/adonis-typings/index.d.ts",
  "author": "virk,adonisjs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/adonis-validation-provider.git"
  },
  "keywords": [
    "validator",
    "schema-validation",
    "adonis",
    "indicative"
  ],
  "bugs": {
    "url": "https://github.com/adonisjs/adonis-validation-provider/issues"
  },
  "homepage": "https://github.com/adonisjs/adonis-validation-provider#readme",
  "nyc": {
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "node ./node_modules/@adonisjs/mrm-preset/validateCommit/conventional/validate.js"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  },
  "publishConfig": {
    "access": "public",
    "tag": "alpha"
  },
  "devDependencies": {
    "@adonisjs/bodyparser": "^3.2.4",
    "@adonisjs/encryption": "^1.0.4",
    "@adonisjs/fold": "^6.3.3",
    "@adonisjs/http-server": "^1.8.0",
    "@adonisjs/logger": "^1.1.9",
    "@adonisjs/mrm-preset": "^2.2.4",
    "@adonisjs/profiler": "^2.0.0",
    "@hapi/joi": "^17.1.0",
    "@types/node": "^13.7.7",
    "benchmark": "^2.1.4",
    "class-validator": "^0.11.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.1.0",
    "dedent": "^0.7.0",
    "del-cli": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-adonis": "^1.0.8",
    "execa": "^4.0.0",
    "husky": "^4.2.1",
    "indicative": "^7.4.4",
    "indicative-compiler": "^7.2.4",
    "japa": "^3.0.1",
    "kleur": "^3.0.3",
    "mrm": "^2.1.0",
    "np": "^5.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@poppinss/utils": "^2.1.2",
    "@types/luxon": "^1.21.0",
    "@types/validator": "^12.0.1",
    "endent": "^1.4.1",
    "lodash.get": "^4.4.2",
    "luxon": "^1.22.0",
    "validator": "^12.2.0"
  }
}
